{
  "name": "react-dev-setup",
  "version": "0.0.1",
  "description": "A React development setup ",
  "main": "index.js",
  "scripts": {
    "predev": "babel-node buildScripts/startMessage.js",
    "dev": "npm-run-all --parallel test:watch lint:watch run:src",
    "test": "mocha --reporter spec buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "lint": "node_modules/.bin/esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "prebuild": "npm run clean-dist & npm run test",
    "build": "babel-node buildScripts/build.js",
    "preprod": "npm run build",
    "prod": "npm run start",
    "run:src": "babel-node buildScripts/srcServer.js",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "start": "babel-node buildScripts/distServer.js"
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  },
  "author": "Patrick McDevitt",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.8.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "bootstrap": "3.3.6",
    "express": "^4.15.3",
    "jquery": "2.2.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "15.4.2",
    "react-router": "2.4.0",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-register": "^6.8.0",
    "chai": "^4.0.2",
    "compression": "^1.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^2.1.11",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.19.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^8.5.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "npm-run-all": "^1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.0.2",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-mock-store": "^1.0.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.7",
    "standard": "^10.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-md5-hash": "0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PMcDevitt/react-dev-setup.git"
  },
  "bugs": {
    "url": "https://github.com/PMcDevitt/react-dev-setup/issues"
  },
  "homepage": "https://github.com/PMcDevitt/react-dev-setup#readme"
}
